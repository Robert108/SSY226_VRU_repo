@page
@model IndexModel
@{
    ViewData["Title"] = "DenCity VehicleSim";
}
<div class="row">
    <div class="col-md-3">

    </div>
    <div class="col-md-6">
        <div class="list-group">
        @foreach (var scenario in Model.Scenarios)
        {
            <a href="~/Index?playback=@scenario.Name"
                class="list-group-item list-group-item-action">
                @scenario.Name - @scenario.Description</a>
        }
        </div>
    </div>
    <div class="col-md-3">

    </div>
</div>
